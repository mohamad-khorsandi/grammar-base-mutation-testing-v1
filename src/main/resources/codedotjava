class MyClass {
    public static void main(String[] args) {
        int x = 5;
        for (int i = 0; i < 0; i++) {
            System.out.println();
        }

        for (int i = 0; i < 0; i--) {
            System.out.println();
            int x = 534;
            int i = 012;
        }
    }
}
