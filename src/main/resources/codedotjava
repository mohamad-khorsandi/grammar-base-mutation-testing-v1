class MyClass {
    public static void main(String[] args) {
        boolean a = true && true || false;
        boolean b = true;
        boolean c = false;
        if (a == true) {
            a = false;
        }
        else if (a == false) {
            a = true || false;
        }
        else {
            a = true;
        }
    }
}
